{
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageReporters": ["lcov", "text-summary"],
    "collectCoverageFrom": ["src/ ** / *. ts"],
    "coveragePathIgnorePatterns": ["/node_modules/", "/_tests_/"],
    "testPathIgnorePatterns": ["/node_modules/"]
}